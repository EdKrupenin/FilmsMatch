        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:statusBarColor">?attr/colorOnBackground</item>
        <item name="android:windowBackground">?attr/colorOnBackground</item>
        <item name="android:windowEnterAnimation">@anim/slide_in_right_dialog</item>
        <item name="android:windowExitAnimation">@anim/slide_out_left_dialog</item>
        <item name="colorPrimary">?attr/colorOnBackground</item>
    </style>

